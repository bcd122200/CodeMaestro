f=open("/share/data/login.txt")
D={}
lines=f.readlines()
for i in lines:

        parts=i.split()
        if len(parts)==2:
                D[parts[0]]=parts[1]
        if len(parts)==3:
                D[parts[0]]=parts[1]+parts[2]


j=raw_input("username: ")
if j in D:
	k=raw_input("password: ")
	if k==D[j]:
		print "You have logged in successfully!"
	else:
		print "password incorrect"
		exit()
else:
	print "login incorrect"
	exit()
